ktime_t start_time, finish_time;
s64 usecs64;
int usecs;

start_time = ktime_get();


finish_time = ktime_get();
usecs64 = ktime_to_ns(ktime_sub(finish_time, start_time));
do_div(usecs64, NSEC_PER_USEC);
usecs = usecs64;
if (usecs == 0)
	usecs = 1;
pr_warn("msec=%ld.%03ld\n", usecs / USEC_PER_MSEC, usecs % USEC_PER_MSEC);
